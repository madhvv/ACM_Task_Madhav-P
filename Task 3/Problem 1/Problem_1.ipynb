        "'''code 1'''\n",
        "test=int(input(\"enter number of testcases:- \"))\n",
        "\n",
        "while test>0:\n",
        "  a=input(\"Enter list:- \")\n",
        "  arr=a.split()\n",
        "\n",
        "  ar=[int(i) for i in arr]\n",
        "\n",
        "  sum=0\n",
        "  for i in range(len(ar)):\n",
        "    var=ar.pop()\n",
        "    if var%2!=0:\n",
        "      sum+=var\n",
        "  print(sum)\n",
        "  test-=1"
